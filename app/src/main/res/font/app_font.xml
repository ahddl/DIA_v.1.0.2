<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--bold-->
    <font
        android:font="@font/scdreambold"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/scdreambold"
        app:fontStyle="normal"
        app:fontWeight="1000"
        tools:targetApi="o" />

    <!--medium-->
    <font
        android:font="@font/scdreammedium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/scdreammedium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:targetApi="o" />

    <!--regular-->
    <font
        android:font="@font/scdreamlight"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/scdreamlight"
        app:fontStyle="normal"
        app:fontWeight="300"
        tools:targetApi="o" />

</font-family>